{
  "name": "mf-app1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "tsc --noEmit && eslint .",
    "start": "craco start",
    "startp": "read -p 'Press Enter to continue...' && craco start",
    "startlogs": "node start-with-pause.js",
    "build": "craco build",
    "test": "craco test",
    "serve": "npm run build && serve -s build -p 3001"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
